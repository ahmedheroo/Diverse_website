
@{
    Layout = null;
}

<!DOCTYPE html>
 
<html lang="en">
 
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="Diverse" content="Diverse Company">
    <meta name="Diverse" content="Diverse Company">
    <meta name="description" content="Explore our diverse range of projects and discover valuable resources, helpful tips, and company updates on our informative blog.">
    <meta name="keywords" content="projects, blog, company updates, resources">
    <title>Diverse - @ViewData["Not Found"]</title>

    <link rel="icon" href="~/client/assets/images/diverse logo cut circle.png">

    <!--begin::Fonts(mandatory for all pages)-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700" />        <!--end::Fonts-->
    <!--begin::Global Stylesheets Bundle(mandatory for all pages)-->
    <link href="~/admin/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/admin/assets/css/style.bundle.css" rel="stylesheet" type="text/css" />
   
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" class="app-blank app-blank bgi-size-cover bgi-position-center bgi-no-repeat">
  
    <div class="d-flex flex-column flex-root" id="kt_app_root">
        <!--begin::Page bg image-->
        <style>
            body {
                background-image: url('~/admin/assets/media/auth/bg12.jpg');
            }

            [data-bs-theme="dark"] body {
                background-image: url('~/admin/assets/media/auth/bg12-dark.jpg');
            }
        </style>
        <!--end::Page bg image-->
        <!--begin::Authentication - Signup Welcome Message -->
        <div class="d-flex flex-column flex-center flex-column-fluid">
            <!--begin::Content-->
            <div class="d-flex flex-column flex-center text-center p-10">
                <!--begin::Wrapper-->
                <div class="card card-flush w-lg-650px py-5">
                    <div class="card-body py-15 py-lg-20">

                        <!--begin::Logo-->
                        <div class="mb-13">
                            <a asp-action="Index" asp-controller="Home" class="">
                                <img alt="Logo" src="~/client/assets/images/diverse logo cut.png" class="h-40px" />
                            </a>
                        </div>
                      
                        <h1 class="fw-bolder text-gray-900 mb-7">
                            Oops!
                        </h1>

                        <div class="fw-semibold fs-6 text-gray-500 mb-7">
                            We can't find that page.

                        </div>

                        <div class="fv-row text-start">
                            <div class="d-flex flex-column flex-md-row justify-content-center gap-3">

                                <a class="btn btn-primary text-nowrap" asp-action="Index" asp-controller="Home">
                                    Return to Home
                                </a>

                            </div>
                        </div>

                        <!--end::Form-->
                        <!--begin::Illustration-->
                        <div class="mb-n5">
                            <img src="~/admin/assets/media/auth/coming-soon.png" class="mw-100 mh-300px theme-light-show" alt="" />
                            <img src="~/admin/assets/media/auth/coming-soon-dark.png" class="mw-100 mh-300px theme-dark-show" alt="" />
                        </div>
                        <!--end::Illustration-->

                    </div>
                </div>
                <!--end::Wrapper-->
            </div>
            <!--end::Content-->
        </div>
      
    </div>
    <!--end::Root-->
    <!--begin::Javascript-->
   
    <script src="~/admin/assets/plugins/global/plugins.bundle.js"></script>
    <script src="~/admin/assets/js/scripts.bundle.js"></script>
   

</body>
 </html>