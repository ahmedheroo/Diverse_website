            <hr class="space m">
            <form action="" class="form-box form-ajax" method="post">
                <div class="row">
                    <div class="col-md-4">
                        <p>Name</p>
                        <!-- <p>اسمك</p> -->
                        <input id="name" name="name" placeholder="" type="text" class="form-control form-value in-bg" required="">
                    </div>
                    <div class="col-md-4">
                        <p>Email</p>
                        <!-- <p>بريد إلكتروني</p> -->
                        <input id="email" name="email" placeholder="" type="email" class="form-control form-value in-bg" required="">
                    </div>
                    <div class="col-md-4">
                        <p>Phone</p>
                        <!-- <p>رقم التليفون</p> -->
                        <input id="phone" name="phone" placeholder="" type="text" class="form-control form-value in-bg" required>
                    </div>
                </div>
                <hr class="space s">
                <div class="row">
                    <div class="col-md-12">
                        <p>Your message</p>
                        <!-- <p>رسالتك</p> -->
